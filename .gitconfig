[user]
	name = Bianca Reis
	email = bia.rbras@hotmail.com
[alias]
	br = branch
	ci = commit -m
	cia = commit --amend
	cian = commit --amend --no-edit
	co = checkout
	df = diff
	dfc = diff --cached
	lo = log --oneline
	rs = reset
	sh = stash
	shc = stash clear
	shl = stash list
	shp = stash pop
	st = status
[core]
	editor = vim
[color "status"]
	untracked = red
	changed = blue
	added = green
